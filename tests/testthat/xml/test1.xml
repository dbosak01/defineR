<MethodDef OID="DM.AGE" Name="Algorithm to compute age" Type="Computation">
  <Description>
    <TranslatedText xml:lang="en">integer value of (BRTHDTC - RFSTDTC)/365.25</TranslatedText>
  </Description>
</MethodDef>
<MethodDef OID="DM.AGEU" Name="Age units hardcode" Type="Computation">
