    <TranslatedText xml:lang="en">Unique sequence based on sort order of studyid usubjid aedecod aestdtc aeendtc</TranslatedText>
  </Description>
</MethodDef>
<MethodDef OID="AE.EPOCH" Name="EPOCH hardcode" Type="Computation">
